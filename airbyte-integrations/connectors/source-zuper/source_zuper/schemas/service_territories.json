{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "no_of_jobs": { 
      "type": "number"
    },
    "is_active": { 
      "type": "boolean"
    },
    "is_deleted": { 
      "type": "boolean"
    },
    "property_name": { 
      "type": "string"
    },
    "property_address": {
      "type": "object",
      "properties": {
        "street": { 
          "type": "string"
        },
        "state": { 
          "type": "string"
        },
        "country": { 
          "type": "string"
        },
        "zip_code": { 
          "type": "string"
        },
        "city": { 
          "type": "string"
        },
        "geo_cordinates": {
          "type": "array",
          "items": {
            "type": "number"
          }
        }
      }
    },
    "property_customers": {
      "type": "array",
      "items": {
        "customer": {
          "type": "object",
          "properties": {
            "customer_last_name": { 
              "type": "string"
            },
            "customer_email": { 
              "type": "string"
            },
            "customer_first_name": { 
              "type": "string"
            },
            "customer_uid": { 
              "type": "string"
            },
            "customer_contact_no": {
              "type": "object",
              "properties": {
                "mobile": { 
                  "type": "string"
                },
                "home": { 
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "custom_fields": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "hide_to_fe": {
            "type": "boolean"
          },
          "label": {
            "type": "string"
          },
          "value": {
            "type": "string"
          }
        }
      }
    },
    "property_image": {
      "type": "string"
    },
    "property_organization": {
      "type": "object",
      "properties": {
        "organization_name": {
          "type": "string"
        },
        "organization_logo": {
          "type": "string"
        },
        "organization_uid": {
          "type": "string"
        }
      }
    },
    "property_uid": {
      "type": "string"
    },
    "created_by": {
      "type": "object",
      "properties": {
        "user_uid": {
          "type": "string"
        },
        "first_name": {
          "type": "string"
        },
        "last_name": {
          "type": "string"
        },
        "email": {
          "type": "string"
        },
        "home_phone_number": {
          "type": "string"
        },
        "designation": {
          "type": "string"
        },
        "emp_code": {
          "type": "string"
        },
        "work_phone_number": {
          "type": "string"
        },
        "profile_picture": {
          "type": "string"
        }
      }
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time"
    }
  }
}
