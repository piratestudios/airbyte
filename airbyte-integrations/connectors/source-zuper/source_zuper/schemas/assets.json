{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "asset_location": {
      "type": "object",
      "properties": {
        "street": {
          "type": "string"
        },
        "city": {
          "type": "string"
        },
        "state": {
          "type": "string"
        },
        "zip_code": {
          "type": "string"
        }
      }
    },
    "useful_life": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string"
          },
          "value": {
            "type": "string"
          }
        }
    },
    "asset_description": {
      "type": "string"
    },
    "parent_asset": {
      "type": "string"
    },
    "asset_quantity": {
      "type": "number"
    },
    "purchase_date": {
      "type": "string",
      "format": "date-time"
    },
    "warranty_expiry_date": {
      "type": "string",
      "format": "date-time"
    },
    "placed_in_service": {
      "type": "string",
      "format": "date-time"
    },
    "asset_code": {
      "type": "string"
    },
    "asset_name": {
      "type": "string"
    },
    "asset_category": {
      "type": "string"
    },
    "asset_barcode": {
      "type": "string"
    },
    "asset_serial_number": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "asset_inspection_form": {
      "type": "string"
    },
    "owned_by_customer": {
      "type": "string"
    },
    "purchase_price": {
      "type": "number"
    },
    "residual_price": {
      "type": "string"
    },
    "customer": {
      "type": "string"
    },
    "asset_product": {
      "type": "string"
    },
    "asset_parts": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "product_id": {
            "type": "string"
          }
        }
      }
    },
    "asset_attachments": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "file_name": {
            "type": "string"
          },
          "url": {
            "type": "string"
          }
        }
      }
    },
    "custom_fields": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "label": {
            "type": "string"
          },
          "value": {
            "type": "string"
          }
        }
      }
    }
  }
}
