    "profile_picture": {
      "type": "string"
    },
    "is_active": {
      "type": "boolean"
    },
    "is_deleted": {
      "type": "boolean"
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time"
    },
