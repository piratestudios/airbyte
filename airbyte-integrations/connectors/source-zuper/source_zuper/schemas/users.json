{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "user_uid": {
      "type": "string"
    },
    "first_name": {
      "type": "string"
    },
    "last_name": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "home_phone_number": {
      "type": ["string", "null"]
    },
    "designation": {
      "type": "string"
    },
    "emp_code": {
      "type": "string"
    },
    "work_phone_number": {
      "type": ["string", "null"]
