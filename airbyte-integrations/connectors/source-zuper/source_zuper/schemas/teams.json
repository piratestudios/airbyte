{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "team_uid": {
      "type": "string"
    },
    "team_name": {
      "type": "string"
    },
    "team_color": {
      "type": "string"
    },
    "team_description": {
      "type": ["string", "null"]
    },
    "user_count": {
      "type": "number"
    },
    "is_active": {
      "type": "number"
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time"
    }
  }
}
