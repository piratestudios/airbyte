{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "transaction_uid": {
            "type": "string"
        },
        "transaction_type": {
            "type": "string"
        },
        "module_name": {
            "type": "string"
        },
        "type": {
            "type": "string"
        },
        "product": {
            "type": "object",
            "properties": {
                "product_uid": {
                    "type": "string"
                },
                "product_name": {
                    "type": "string"
                }
            }
        },
        "to_location": {
            "type": ["object", "null"],
            "properties": {
                "location_uid": {
                    "type": "string"
                },
                "location_name": {
                    "type": "string"
                }
            }
        },
        "remarks": {
            "type": "string"
        },
        "quantity": {
            "type": "number"
        },
        "serial_nos": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "purchase_price": {
            "type": ["number", "null"]
        },
        "is_deleted": {
            "type": "boolean"
        },
        "created_by": {
            "type": "object",
            "properties": {
                "user_uid": {
                    "type": "string"
                },
                "emp_code": {
                    "type": "string"
                },
                "prefix": {
                    "type": ["string", "null"]
                },
                "first_name": {
                    "type": "string"
                },
                "last_name": {
                    "type": "string"
                },
                "email": {
                    "type": "string"
                },
                "designation": {
                    "type": "string"
                },
                "home_phone_number": {
                    "type": ["string", "null"]
                },
                "work_phone_number": {
                    "type": ["string", "null"]
                },
                "mobile_phone_number": {
                    "type": ["string", "null"]
                },
                "profile_picture": {
                    "type": "string"
                },
                "is_active": {
                    "type": "boolean"
                },
                "is_deleted": {
                    "type": "boolean"
                },
                "role": {
                    "type": "object",
                    "properties": {
                        "role_id": {
                            "type": "number"
                        },
                        "role_uid": {
                            "type": "string"
                        },
                            "role_name": {
                            "type": "string"
                        },
                            "role_key": {
                            "type": "string"
                        },
                        "created_at": {
                            "type": "string",
                            "format": "date-time"
                        },
                        "updated_at": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "created_at": {
            "type": "string",
            "format": "date-time"
        }
    }
}
